[
    {
        "name": "create_schedule",
        "description": "增加、新建个人日程安排的工具，在需要增加、创建、新建新的个人日程时使用本工具；同时会查出新日程开始时间当天的全部日程",
        "parameters": {
            "type": "object",
            "properties": {
                "startTime": {
                    "description": "日程开始时间，格式为YYYY-MM-DD hh:mm:ss",
                    "type": "string"
                },
                "endTime": {
                    "description": "日程结束时间，格式为YYYY-MM-DD hh:mm:ss",
                    "type": "string"
                },
                "title": {
                    "description": "日程内容",
                    "type": "string"
                }
            },
            "required": ["startTime", "endTime", "title"]
        },
        "results": {
            "type": "object",
            "properties": {
                "startTime": {
                    "description": "日程开始时间，格式为YYYY-MM-DD hh:mm:ss",
                    "type": "string"
                },
                "endTime": {
                    "description": "日程结束时间，格式为YYYY-MM-DD hh:mm:ss",
                    "type": "string"
                },
                "title": {
                    "description": "日程内容",
                    "type": "string"
                }
            }
        }
    }
]
